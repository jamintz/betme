
<h1>Guesses</h1>

<table>
  <thead>
    <tr>
      <th>Prop</th>
      <th>Correct</th>
      <th>Title</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @guesses.each do |guess| %>
      <tr>
        <td><%= guess.prop_id %></td>
        <td><%= guess.correct %></td>
        <td><%= guess.title %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Guess', new_guess_path %>
